C-M-[Backspace] # log out x window
C-M-[F1~F6] # switch to terminal
C-M-[F7] # back to x window
startx # start x window
bc # calculator
netstat # connection status
sync # flush file system buffers

who
exit
date
cal
info 
nano
ps -aux

shutdown, reboot, halt, poweroff
pwd, cd, ls, ll, cp, mv, rm, mkdir


inxi -Fxz
tar -xzfv
ps aux | grep

wget --recursive --no-parent / wget -r -np
wget -E -H -k -K -p (-e robots=off)

exec bash -l
